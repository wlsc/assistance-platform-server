package de.tudarmstadt.informatik.tk.assistanceplatform.services.messaging;

public abstract class MessagingService {
	public MessagingService() {
		
	}
	
	protected Channel channel(String name) {
		
	}
	
	protected abstract void subscribe(Subscription subscription);
	
	protected abstract void unsubscribe(Subscription subscription);
	
	protected abstract void publish(String subscriptionName, Object data);
}